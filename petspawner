-- Make sure Delta has proper permissions to access ReplicatedStorage
local petEvent = game:GetService("ReplicatedStorage"):WaitForChild("PetModule"):WaitForChild("SpawnPet")

-- Change "Raccoon" to the pet name you want to spawn
local petName = "Raccoon"

-- Number of pets to spawn
local count = 5

for i = 1, count do
    petEvent:FireServer(petName)
    wait(0.25) -- slight delay to avoid detection or server lag
end
