local CmdrFunction = game:GetService("ReplicatedStorage"):WaitForChild("CmdrClient"):WaitForChild("CmdrFunction")

local oldInvoke
oldInvoke = hookfunction(CmdrFunction.InvokeServer, function(self, ...)
    print("[üîç CmdrFunction Triggered]")
    local args = {...}
    for i, v in ipairs(args) do
        print("Arg[" .. i .. "]:", v)
    end
    return oldInvoke(self, ...)
end)

print("‚úÖ Hooked CmdrFunction - waiting for triggers...")
